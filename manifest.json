{
  "manifest_version": 2,
  "name": "Instagram Adjust",
  "version": "1.0",
  "description": "This extention ajust size of photos on instagram page",
  "permissions": ["tabs", "*://*.instagram.com/*"],
  "content_scripts": [
    {
      "matches": ["*://*.instagram.com/*"],
      "js": ["background.js"]
    }
  ],
  
  "browser_action": {
    "default_icon": "inst-icon.png"
},
	
  "icons" : {
		"16" : "inst-icon.png",
		"48" : "inst-icon.png",
		"128" : "inst-icon.png"
	}
}